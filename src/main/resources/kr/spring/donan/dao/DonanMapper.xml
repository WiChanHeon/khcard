<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="kr.spring.donan.dao.DonanMapper">  
	<select id="selectBoardList" parameterType="hashmap" resultType="DonanCommand">
	SELECT
	loss_num,
	card_num,
	loss_reg,
	loss_memo
	FROM
	f_loss
</select>

</mapper>